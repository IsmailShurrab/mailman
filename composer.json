{
    "name": "waavi/mailman",
    "version":  "1.0",
    "description": "Easier email sending for Laravel",
    "authors": [
        {
            "name": "Waavi",
            "email": "info@waavi.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.x",
        "illuminate/mail": "4.x",
        "tijsverkoyen/css-to-inline-styles": "1.2.*"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Waavi\\Mailman": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}